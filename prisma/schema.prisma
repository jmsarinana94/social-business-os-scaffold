generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id           String   @id @default(cuid())
  orgId        String
  title        String
  type         String
  description  String?
  price        Int
  sku          String?
  inventoryQty Int
  status       String
  createdAt    DateTime @default(now())
}
