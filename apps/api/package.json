{
  "name": "@repo/api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/src/main.js",
  "scripts": {
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test": "jest --config jest.config.ts",
    "test:e2e": "jest --config jest.config.ts --selectProjects=e2e --runInBand",
    "test:e2e:products": "jest --config jest.config.ts --selectProjects=e2e --runInBand -- test/e2e/products.validation.e2e-spec.ts",
    "inv:test": "bash ./scripts/inventory-test.sh",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:reset": "bash ./scripts/prisma-reset.sh",
    "seed:demo": "bash ./scripts/prisma-seed-demo.sh",
    "dev": "bash ./scripts/dev.sh"
  },
  "dependencies": {
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.4.20",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "express-rate-limit": "^7.5.1",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
