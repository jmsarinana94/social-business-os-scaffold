let AuthController = class AuthController {
    constructor(auth) {
        this.auth = auth;
    }
    async signup(org, dto) {
        const token = await this.auth.signup({
            org: org || 'demo',
            email: dto.email,
            password: dto.password,
        });
        return { access_token: token };
    }
    async login(org, dto) {
        const token = await this.auth.login({
            org: org || 'demo',
            email: dto.email,
            password: dto.password,
