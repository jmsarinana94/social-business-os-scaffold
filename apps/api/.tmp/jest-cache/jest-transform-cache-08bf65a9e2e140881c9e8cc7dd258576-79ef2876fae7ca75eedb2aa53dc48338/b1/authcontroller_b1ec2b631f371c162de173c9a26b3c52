const common_1 = require("@nestjs/common");
const auth_service_1 = require("./auth.service"); // â† named import
const jwt_auth_guard_1 = require("./guards/jwt-auth.guard");
let AuthController = class AuthController {
    constructor(auth) {
        this.auth = auth;
    }
    // POST /auth/signup -> 201 Created
    async signup(org, dto) {
        const slug = (org ?? process.env.ORG ?? 'demo').trim();
        return this.auth.signup({ org: slug, email: dto.email, password: dto.password });
    }
    // POST /auth/login -> 200 OK
    async login(org, dto) {
        const slug = (org ?? process.env.ORG ?? 'demo').trim();
        return this.auth.login({ org: slug, email: dto.email, password: dto.password });
    }
    // GET /auth/me (Bearer token required)
    async me(req) {
        // JwtStrategy should set req.user = { sub, email, iat, exp, ... }
        const userId = req?.user?.sub;
        return this.auth.me(userId);
    }
};
exports.AuthController = AuthController;
