let AuthController = class AuthController {
    constructor(auth) {
        this.auth = auth;
    }
    getOrgSlug(headers) {
        const slug = headers['x-org'];
        if (!slug)
            throw new common_1.BadRequestException('Missing x-org header');
        return String(slug);
    }
    async signup(headers, dto) {
        const slug = this.getOrgSlug(headers);
        return this.auth.signup({ org: slug, email: dto.email, password: dto.password, name: dto.name });
    }
    async login(headers, dto) {
        const slug = this.getOrgSlug(headers);
        return this.auth.login({ org: slug, email: dto.email, password: dto.password });
    }
    async me(req) {
        // JwtStrategy.validate returns { userId, email }
        const userId = req.user?.userId;
        if (!userId)
            throw new common_1.BadRequestException('Invalid token');
        return this.auth.me(userId);
    }
};
