const common_1 = require("@nestjs/common");
const jwt_1 = require("@nestjs/jwt");
const bcrypt = __importStar(require("bcryptjs"));
const prisma_service_1 = require("../../shared/prisma/prisma.service");
let AuthService = class AuthService {
    constructor(prisma, jwt) {
        this.prisma = prisma;
        this.jwt = jwt;
    }
    async signup(dto) {
        const orgSlug = dto.org?.trim() || 'demo';
        const org = (await this.prisma.organization.findUnique({ where: { slug: orgSlug } })) ??
            (await this.prisma.organization.create({
                data: { slug: orgSlug, name: orgSlug },
            }));
        const passwordHash = await bcrypt.hash(dto.password, 10);
        const user = await this.prisma.user.create({
            data: { email: dto.email, password: passwordHash },
        });
        const existingMember = await this.prisma.orgMember.findFirst({
            where: { userId: user.id, orgId: org.id },
        });
        if (!existingMember) {
            await this.prisma.orgMember.create({
                data: { userId: user.id, orgId: org.id, role: 'MEMBER' },
            });
        }
        return { id: user.id, email: user.email };
    }
    async login(dto) {
        const orgSlug = dto.org?.trim() || 'demo';
        const user = await this.prisma.user.findUnique({
            where: { email: dto.email },
        });
        if (!user)
            throw new common_1.UnauthorizedException('Invalid email or password');
        const ok = await bcrypt.compare(dto.password, user.password);
        if (!ok)
            throw new common_1.UnauthorizedException('Invalid email or password');
        // Best-effort lastLoginAt; tolerate older DBs without the column
        try {
            await this.prisma.user.update({
                where: { id: user.id },
                data: { lastLoginAt: new Date() },
            });
        }
        catch {
            // ignore if column missing
        }
        await this.prisma.organization.upsert({
            where: { slug: orgSlug },
            update: {},
            create: { slug: orgSlug, name: orgSlug },
        });
        const payload = { sub: user.id, email: user.email };
        const access_token = await this.jwt.signAsync(payload);
        return { access_token };
    }
};
