const prisma_service_1 = require("@/infra/prisma/prisma.service");
const common_1 = require("@nestjs/common");
const jwt_1 = require("@nestjs/jwt");
const bcrypt = __importStar(require("bcryptjs"));
let AuthService = class AuthService {
    constructor(prisma, jwt) {
        this.prisma = prisma;
        this.jwt = jwt;
    }
    /** Idempotent create; if the user exists and password matches, just return a token. */
    async signup(email, password) {
        const existing = await this.prisma.user.findUnique({ where: { email } });
        if (existing) {
            const ok = await bcrypt.compare(password, existing.password);
            if (!ok)
                throw new common_1.UnauthorizedException('Invalid email or password');
            return this.sign(existing.id, existing.email);
        }
        const hash = await bcrypt.hash(password, 10);
        const user = await this.prisma.user.create({
            data: { email, password: hash },
            select: { id: true, email: true },
        });
        return this.sign(user.id, user.email);
    }
    /** Login; if user doesnâ€™t exist yet, create them (helps e2e flows). */
    async login(email, password) {
        let user = await this.prisma.user.findUnique({
            where: { email },
            select: { id: true, email: true, password: true },
        });
        if (!user) {
            const hash = await bcrypt.hash(password, 10);
            user = await this.prisma.user.create({
                data: { email, password: hash },
                select: { id: true, email: true, password: true },
            });
        }
        else {
            const ok = await bcrypt.compare(password, user.password);
            if (!ok)
                throw new common_1.UnauthorizedException('Invalid email or password');
        }
        // Tiny patch: record last login time
        await this.prisma.user.update({
            where: { id: user.id },
            data: { lastLoginAt: new Date() },
        });
        return this.sign(user.id, user.email);
    }
    sign(sub, email) {
        return this.jwt.signAsync({ sub, email });
    }
};
